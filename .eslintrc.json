{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "extends": ["eslint:recommended"],
  "rules": {
    "no-unused-vars": "off",
    "no-restricted-syntax": [
      "error",
      {
        "selector": "Property[key.name=\"secretKey\"]",
        "message": " 砖 专: 住专 砖转砖 驻转 'secretKey'!"
      },
      {
        "selector": "Property[key.name=\"debugMode\"]",
        "message": " 砖 专: 住专 砖转砖 驻转 'debugMode'!"
      },
      {
        "selector": "MemberExpression[property.name=\"debugMode\"]",
        "message": " 砖 专: 住专 砖转砖 转 'debugMode'!"
      },
      {
        "selector": "VariableDeclarator[id.name=\"debugMode\"]",
        "message": " 砖 专: 住专 专 砖转 砖 'debugMode'!"
      },
      {
        "selector": "VariableDeclarator[id.name=\"secretKey\"]",
        "message": " 砖 专: 住专 专 砖转 砖 'secretKey'!"
      }
    ],
    "no-restricted-properties": [
      "error",
      {
        "object": "config",
        "property": "debugMode",
        "message": " 砖 专: 住专 砖转砖 -config.debugMode!"
      }
    ]
  }
}